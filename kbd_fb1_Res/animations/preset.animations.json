{
	"format_version": "1.8.0",
	"animations": {
		"animation.preset.projectile.base": {
			"loop": true,
			"bones": {
				"root": {
					"rotation": ["variable.shake_power - query.target_x_rotation", "-query.target_y_rotation", 0]
				}
			}
		},
		"animation.preset.projecbase": {
			"loop": true,
			"bones": {
				"root": {
					"rotation": ["variable.shake_power - query.target_x_rotation", "-query.target_y_rotation", 0]
				}
			}
		},
		"animation.preset.root.scale": {
			"loop": true,
			"bones": {
				"root": {
					"scale": "variable.root_scale"
				}
			}
		},
		"animation.preset.root.position": {
			"loop": true,
			"bones": {
				"root": {
					"position": ["variable.root_position_x", "variable.root_position_y", "variable.root_position_z"]
				}
			}
		},
        "animation.preset.root.rotation": {
			"loop": true,
			"bones": {
				"root": {
					"position": ["variable.root_rotation_x", "variable.root_rotation_y", "variable.root_rotation_z"]
				}
			}
		},
        "animation.preset.root.display_float": {
			"loop": true,
			"bones": {
				"root": {
					"rotation": [0, "query.life_time * 15", 0],
					"position": [0, "math.sin(query.life_time * 60) * 5", 0]
				}
			}
		}
    }
}